// navigation bar
/* navbar */
.navbar-default
    background-color font-color
    color back-color

/* Link colors */
.navbar-nav > li > a 
    color #ffffff
    text-decoration none

.navbar-nav > li > a:hover,
.navbar-nav > li > a:focus 
    color #ffffff
    outline 0

.navbar-nav > .active > a,
.navbar-nav > .active > a:hover,
.navbar-nav > .active > a:focus 
    color #f8ffff
    background-color #40a0af
    outline 0
